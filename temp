    if entity_id is not None:
        headers = {'Accept': 'application/json'}
        auth = HTTPBasicAuth('X-Api-Key', key)
        req = requests.get(url, headers=headers, auth=auth)
        data = req.json()

        options = []

        for file in data["files"]:
            options.append(file['name'])

        #service_data = {"entity_id": entity_id, "options": options}
        input_select.set_options(options)